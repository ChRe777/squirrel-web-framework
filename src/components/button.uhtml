<div id="Button_x4rt54"></div>
<script type="module">

import { render, html, signal } from 'https://cdn.jsdelivr.net/npm/uhtml/preactive.js';
const count = signal(0);
const elem = document.getElementById("Button_x4rt54");
    render(elem, () => html`
<button class="btn btn-primary" onclick=${() => { count.value++ }}>
   Clicks: ${count.value}
</button>
    `);
</script>
